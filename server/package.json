    "clean": "rimraf dist"
  },
  "dependencies": {
    "bull": "^3.7.0",
    "concurrently": "^4.1.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.1",
    "express-session": "^1.15.6",
    "get-youtube-title": "^1.0.0",
    "moment": "^2.28.0",
    "mongoose": "^5.7.5",
    "nodemon": "^1.18.8",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "passport.socketio": "^3.7.0",
    "socket.io": "^2.0.4",
    "typescript": "^3.4.1",
    "updated-youtube-info": "1.4.7"
  },
  "devDependencies": {
    "@types/bull": "^3.5.11",
    "@types/connect-mongo": "0.0.41",
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.26",
    "@types/node": "^11.15.21",
    "@types/socket.io": "^2.1.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "forever": "^1.0.0",
    "rimraf": "^2.6.2"
  }
}
