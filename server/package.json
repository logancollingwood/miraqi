{
  "name": "miraqi-api",
  "version": "1.0.0",
  "description": "The backend socket api for interacting with Miraqi Services",
  "author": "logan collingwood",
  "license": "",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "forever dist/server/src/index.js",
